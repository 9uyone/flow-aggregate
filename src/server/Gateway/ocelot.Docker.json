{
	"Routes": [
		{
			"DownstreamPathTemplate": "/collector/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "collector",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/collector/{everything}",
			"UpstreamHttpMethod": [ "GET", "POST" ]
		},
		{
			"DownstreamPathTemplate": "/source/{src}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "storage",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/source/{src}",
			"UpstreamHttpMethod": [ "GET" ],
			"AuthenticationOptions": {
				"AuthenticationProviderKey": "Bearer",
				"AllowedScopes": []
			}
		},
		{
			"DownstreamPathTemplate": "/parser-cfg/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "storage",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/parser-cfg/{everything}",
			"UpstreamHttpMethod": [ "GET", "POST", "PATCH", "DELETE" ],
			"AuthenticationOptions": {
				"AuthenticationProviderKey": "Bearer",
				"AllowedScopes": []
			}
		},
		{
			"DownstreamPathTemplate": "/auth/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "auth",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/auth/{everything}",
			"UpstreamHttpMethod": [ "POST", "GET" ]
		},
		{
			"DownstreamPathTemplate": "/hangfire/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "scheduler",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/hangfire/{everything}",
			"UpstreamHttpMethod": [ "POST", "GET" ]
		}
	],
	"GlobalConfiguration": {
		"BaseUrl": "https://localhost:8081"
	}
}